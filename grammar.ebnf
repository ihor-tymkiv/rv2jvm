program         ::= (label | (instruction '\n'))*
instruction     ::= identifier (symbol (',' symbol)*)?
label           ::= identifier ':'
symbol          ::= register | identifier | number | memory_operand
memory_operand  ::= number? '(' register ')'
register        ::= 'x' ([0-9] | [12][0-9] | 3[01])
identifier      ::= [_a-zA-Z][_a-zA-Z0-9\.]*
number          ::= '-'? decimal
decimal         ::= [1-9][0-9]*
